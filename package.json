{
  "name": "@underdogio/redux-rest-data",
  "version": "0.1.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/underdogio/redux-rest-data.git"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "yarn clean && tsc --outDir dist",
    "clean": "rm -rf dist",
    "docs": "typedoc --mode modules --out ./docs/",
    "example": "parcel ./example/index.html",
    "prepublishOnly": "yarn build",
    "test": "nyc --reporter=text ava",
    "test/watch": "yarn test --watch"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "reporter": [
      "text-summary",
      "html"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.5",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "^7.0.10",
    "ava": "^1.3.1",
    "fetch-mock": "^8.0.0",
    "husky": "^1.3.1",
    "node-fetch": "^2.6.0",
    "nyc": "^13.3.0",
    "parcel-bundler": "^1.12.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.2.7",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}
